{% load custom_filters %}<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>NDNP OCR Dumps</title>
    <link rel="self" type="application/atom+xml" href="http://{{ request.get_host }}{% url chronam_ocr_atom %}" />
    <id>info:lc/ndnp/ocr</id>
    <author>
        <name>Library of Congress</name>
        <uri>http://loc.gov</uri>
    </author>
    <updated>{{ now }}</updated>

    {% for dumpfile in dumpfiles %}
    <entry>
        <title>{{ dumpfile.name }}</title>
        <link rel="alternate" type="application/x-bzip2" href="http://{{ request.get_host }}/data/dumps/ocr/{{ dumpfile.name }}" />
        <id>info:lc/ndnp/dump/ocr/{{ dumpfile.name }}</id>
        <updated>{{ dumpfile.updated|rfc3339 }}</updated>
        <summary type="xhtml"><div xmlns="http://www.w3.org/1999/xhtml">OCR dump file <a href="http://{{ request.get_host }}/data/dumps/ocr/{{ dumpfile.name }}">{{ dumpfile.name }}</a> with size {{ dumpfile.size|filesizeformat }} generated {{ dumpfile.updated }}</div></summary>
    </entry>
    {% endfor %}

</feed>

